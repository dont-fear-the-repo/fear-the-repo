{
  "name": "fear-the-repo",
  "version": "0.0.1",
  "description": "Drag-and-drop resume version control tool",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint ./src/**/*.js*",
    "lint:tests": "eslint ./tests/**/*.spec.js",
    "compile": "node -r dotenv/config --harmony bin/compile",
    "start": "npm run dev",
    "dev": "node -r dotenv/config --harmony bin/server",
    "server-dev": "nodemon -r dotenv/config --harmony bin/server",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "node ./node_modules/karma/bin/karma start",
    "test:dev": "npm run test -- --watch",
    "deploy": "npm run lint && npm run test && npm run compile",
    "postinstall": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dont-fear-the-repo/fear-the-repo.git"
  },
  "author": [
    "Andrew Huang <andrew.huang12@gmail.com> (http://github.com/AndrewTHuang)",
